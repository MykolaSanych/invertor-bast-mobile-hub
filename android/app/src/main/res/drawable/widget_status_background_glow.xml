<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="14dp" />
            <solid android:color="#00000000" />
            <stroke
                android:width="3dp"
                android:color="#E600D4FF" />
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape android:shape="rectangle">
            <corners android:radius="12dp" />
            <solid android:color="#00000000" />
            <stroke
                android:width="2dp"
                android:color="#B37DAFFF" />
        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="#00000000" />
            <stroke
                android:width="1dp"
                android:color="#99E1F4FF" />
        </shape>
    </item>
</layer-list>
